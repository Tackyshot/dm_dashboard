{
  "name": "api_and_socket_example",
  "version": "0.0.1",
  "description": "Api and web socket server example that has a rudimentary authentication system and hello world messaging",
  "main": "./dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node ./dist/app.js",
    "build:dev": "rm -rf ./dist && npm run build:server:dev",
    "build:prod": "rm -rf ./dist && npm run build:server",
    "build:server": "rm -rf ./dist && NODE_ENV=PRODUCTION babel src/server -d dist --copy-files",
    "build:server:dev": "rm -rf ./dist && NODE_ENV=DEVELOPMENT babel src/server -d dist --copy-files"
  },
  "author": "business.dillonosman@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/node": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-proposal-decorators": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-function-sent": "^7.2.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.4.4",
    "babel-loader": "^8.0.5"
  },
  "dependencies": {
    "@hapi/cookie": "^10.1.0",
    "bcryptjs": "^2.4.3",
    "blacklist": "^1.1.4",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "hapi": "^18.1.0",
    "hapi-auth-jwt2": "^8.4.0",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.5.5",
    "socket.io": "^2.2.0"
  }
}
